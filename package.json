{
  "name": "superior-solstice",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prettier": "prettier --write 'src/**/*.{ts,astro,scss}'",
    "stylelint": "stylelint --fix 'src/**/*.{astro,scss}'",
    "eslint": "eslint --fix 'src/**/*.{ts,astro}'",
    "prepare": "husky install"
